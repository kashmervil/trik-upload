<Window x:Class="Trik.Upload_Extension.Targets"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Targets" Height="135" Width="300" WindowStartupLocation="CenterScreen">
    <DockPanel Margin="10">
        <StackPanel DockPanel.Dock="Right" Margin="10,0,0,0">
            <Button Name="AddTargetButton" Click="AddTargetButton_OnClick">Add TRIK</Button>
            <Button x:Name="DeleteTargetButton" Click="DeleteTargetButton_OnClick" Content="Delete TRIK" Margin="0,10"/>
        </StackPanel>
        <ListBox Name="ListBoxTargets" ></ListBox>
    </DockPanel>
</Window>
